{
    "name": "full-stack",
    "version": "1.0.0",
    "description": "T-MERN App",
    "main": "dist/server.js",
    "scripts": {
        "build-prod": "npm ci && npm run build && npm run build-types && npm prune --production",
        "heroku-postbuild": "npm run build-prod",
        "prebuild": "rimraf ./dist/*",
        "build": "webpack --config=webpack.config.prod.js",
        "build-dev": "npm run prebuild && npm run build-types && webpack --progress --display-modules --config=webpack.config.dev.js",
        "build-types": "tsc --emitDeclarationOnly --p ./tsconfig.json",
        "type-check": "tsc --noEmit --p ./tsconfig.json",
        "type-check:watch": "npm run type-check -- --watch",
        "serve": "node dist/server.js",
        "build-server-once": "webpack --stack=server-build-once --config=webpack.config.dev.js",
        "watch-client": "webpack-dev-server --stack=client --config=webpack.config.dev.js",
        "watch-server": "webpack --watch --stack=server --config=webpack.config.dev.js",
        "watch": "npm run prebuild && npm run build-server-once && concurrently -k -p \"[{name}]\" -n \"Server,NodeServe,Client\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-server\" \"npm run serve\" \"npm run watch-client\"",
        "server": "npm run prebuild && npm run build-server-once && concurrently -k -p \"[{name}]\" -n \"Server,NodeServe\" -c \"yellow.bold,cyan.bold\" \"npm run watch-server\" \"npm run serve\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "lint-client": "eslint ./client/ --ext .js,.jsx,.ts,.tsx",
        "lint-server": "eslint ./server/ --ext .js,.jsx,.ts,.tsx",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint-rule-timings": "cross-env TIMING=1 npm run lint",
        "start": "npm run serve"
    },
    "keywords": [
        "Webpack",
        "Typescript",
        "Babel",
        "ESLint",
        "ExpressJS",
        "React",
        "NodeJS"
    ],
    "author": "Bang Chi Duong",
    "license": "ISC",
    "dependencies": {
        "@google/markerclustererplus": "^5.0.4",
        "@overnightjs/core": "^1.6.9",
        "@overnightjs/logger": "^1.1.9",
        "axios": "^0.19.2",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "core-js": "^3.6.5",
        "debug": "^4.1.1",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "jsdom": "^16.2.2",
        "lodash": "^4.17.19",
        "lusca": "^1.6.1",
        "path": "^0.12.7",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "regenerator-runtime": "^0.13.5"
    },
    "devDependencies": {
        "@babel/core": "7.7.4",
        "@babel/plugin-proposal-class-properties": "7.7.4",
        "@babel/plugin-proposal-decorators": "7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "7.7.4",
        "@babel/plugin-proposal-optional-chaining": "^7.9.0",
        "@babel/preset-env": "7.7.4",
        "@babel/preset-react": "7.7.4",
        "@babel/preset-typescript": "7.7.4",
        "@types/body-parser": "^1.17.0",
        "@types/compression": "0.0.36",
        "@types/debug": "^4.1.5",
        "@types/dotenv": "^6.1.1",
        "@types/errorhandler": "0.0.32",
        "@types/express": "^4.17.0",
        "@types/googlemaps": "^3.39.3",
        "@types/jest": "^24.0.15",
        "@types/jsdom": "^16.2.1",
        "@types/lodash": "^4.14.136",
        "@types/lusca": "^1.6.0",
        "@types/react": "^16.9.16",
        "@types/react-dom": "^16.9.4",
        "@types/react-router": "^5.1.3",
        "@types/react-router-dom": "^4.3.5",
        "@types/supertest": "^2.0.8",
        "@types/webpack-env": "^1.14.1",
        "@typescript-eslint/eslint-plugin": "^2.13.0",
        "@typescript-eslint/eslint-plugin-tslint": "^2.16.0",
        "@typescript-eslint/parser": "^2.13.0",
        "babel-loader": "8.0.6",
        "concurrently": "^4.1.1",
        "css-loader": "^3.3.2",
